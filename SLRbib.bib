@article{zuiderwijk_open_2014,
	title = {Open data policies, their implementation and impact: A framework for comparison},
	volume = {31},
	issn = {0740624X},
	doi = {10.1016/j.giq2013.04.003},
	shorttitle = {Open data policies, their implementation and impact},
	abstract = {In developing open data policies, governments aim to stimulate and guide the publication of government data and to gain advantages from its use. Currently there is a multiplicity of open data policies at various levels of government, whereas very little systematic and structured research has been done on the issues that are covered by open data policies, their intent and actual impact. Furthermore, no suitable framework for comparing open data policies is available, as open data is a recent phenomenon and is thus in an early stage of development. In order to help bring about a better understanding of the common and differentiating elements in the policies and to identify the factors affecting the variation in policies, this paper develops a framework for comparing open data policies. The framework includes the factors of environment and context, policy content, performance indicators and public values. Using this framework, seven Dutch governmental policies at different government levels are compared. The comparison shows both similarities and differences among open data policies, providing opportunities to learn from each other's policies. The findings suggest that current policies are rather inward looking, open data policies can be improved by collaborating with other organizations, focusing on the impact of the policy, stimulating the use of open data and looking at the need to create a culture in which publicizing data is incorporated in daily working processes. The findings could contribute to the development of new open data policies and the improvement of existing open data policies.},
	pages = {17--29},
	number = {1},
	journaltitle = {Government Information Quarterly},
	shortjournal = {Government Information Quarterly},
	author = {Zuiderwijk, Anneke and Janssen, Marijn},
	date = {2014-01},
	keywords = {{INFORMATION} sharing, {NETHERLANDS}, {GUIDELINES}, {ACCESS} to information, {ACCESS} control, {GOVERNMENT} information, Open data, Open government, Comparative research, Open data framework, Open data policies, Policies, {PUBLIC} records},
}

@article{koerten_public_2013,
	title = {Public sector information reuse across Europe: Patterns in policy-making from an organizational perspective},
	volume = {36},
	issn = {18787673},
	doi = {10.3233/GOV-130356},
	shorttitle = {Public sector information reuse across Europe},
	abstract = {The success of Google Maps suggests that free access to public sector information ({PSI}) contributes to a prosperous economy. Although considerable attention has been paid to the legal and economic aspects of policies on public sector information ({PSI}), also known as open data ({OD}), the organizational consequences regarding the nature and shape of the public sector are rarely anticipated. We develop a framework to analyse organizational settings by comparing policies and outcomes of {PSI} reuse in {EU} member states. In many discussions on {PSI} reuse, government is treated as a unitary phenomenon with a single voice. We found considerable differences among {EU} member states concerning the implementation of {PSI} policies and indications that they are connected with the nature of the public sector in a specific state. Therefore we suggest that policies on {PSI} reuse to stimulate economic prosperity should be developed which take the specific national setting into consideration. When national government develops policies aimed simply at disclosing {PSI} without paying attention to the development of {PSI} reuse arenas, it runs the risk of unleashing narratives of control within the public sector, preventing them from releasing the innovative potential that {PSI} reuse intrinsically possesses.},
	pages = {198--211},
	number = {4},
	journaltitle = {Journal of E-Governance},
	shortjournal = {Journal of E-Governance},
	author = {Koerten, Henk and Veenswijk, Marcel},
	date = {2013-10},
	keywords = {{EUROPE}, {ORGANIZATIONAL} change, {INFORMATION} sharing, {INFORMATION} resources management, {PUBLIC} administration, {INFORMATION} services, {INFORMATION} resources, {GOOGLE} Maps, Public sector information reuse},
}

