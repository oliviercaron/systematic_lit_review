	keywords = {{MARKETING}, {CUSTOMER} loyalty programs, {EMPIRICAL} research, {PROFITABILITY}, {VALUATION}, customer satisfaction, {STOCHASTIC} models, {STOCHASTIC} programming},
}

@article{wu_satisfaction_2012,
