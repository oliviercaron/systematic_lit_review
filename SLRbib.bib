	file = {EBSCO Full Text:C\:\\Users\\33623\\Zotero\\storage\\M5KGANXG\\Schau et al. - 2009 - How Brand Community Practices Create Value.pdf:application/pdf;EBSCO Full Text:C\:\\Users\\33623\\Zotero\\storage\\IB6JHSZ6\\Schau et al. - 2009 - How Brand Community Practices Create Value.pdf:application/pdf},
