@article{chiquan_guo_customer_2004,
	title = {Customer satisfaction and profitability: is there a lagged effect?},
	volume = {12},
	issn = {0965254X},
	url = {http://search.ebscohost.com/login.aspx?direct=true&db=bth&AN=14351956&lang=fr&site=ehost-live},
	doi = {10.1080/0965254042000262878},
	shorttitle = {Customer satisfaction and profitability},
	abstract = {Although customer satisfaction is one of the fundamental concepts in marketing theory and practices, the direct link between customer satisfaction and a firm's bottom line is still somewhat ambiguous and vague. While most research focuses on how customer satisfaction contributes to the firm's profitability, financial resources required to implement satisfaction programs are largely ignored in the literature. This paper takes a conceptual middle ground, proposing that customer satisfaction influences profitability, and at the same time, profitability impacts satisfaction. However, based on recent evidence (Rust, Moorman and Dickson, 2002), we propose that there is a lagged effect between the two. In other words, past satisfaction has a positive effect on current profitability, and similarly, past profitability affects current satisfaction. Furthermore, event study methodology is utilized to examine stock reaction to the announcement of the firm's satisfaction performance, solidifying the relationship between satisfaction, a marketing metric, and financial performance. [{ABSTRACT} {FROM} {AUTHOR}]},
	pages = {129--144},
	number = {3},
	journaltitle = {Journal of Strategic Marketing},
	author = {Chiquan Guo and Kumar, Anand and Jiraporn, Pornsit},
	urldate = {2009-11-25},
	date = {2004},
	keywords = {{MANAGEMENT}, {MARKETING}, {CUSTOMER} relations, {CONSUMER} satisfaction, {PROFITABILITY}, customer satisfaction, {FINANCIAL} performance},
}

