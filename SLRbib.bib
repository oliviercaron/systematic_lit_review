	keywords = {{CONSUMER} behavior, {MARKETING} strategy, {CONSUMERS} -- Attitudes, {MARKETING} research, {CUSTOMER} relations, relationship marketing, {CONSUMERS} -- Research, {CONSUMER} satisfaction, {PROFITABILITY}, Customer loyalty, loyalty, satisfaction, metrics, transactional customers},
}

