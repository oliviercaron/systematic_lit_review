	date = {2007},
	keywords = {{MANAGEMENT}, {CUSTOMER} services, {RESEARCH}, {TOURISM}, {CONSUMER} satisfaction, {COST}, customer satisfaction, Service industries, {SERVICE} industries -- Management},
	file = {EBSCO Full Text:C\:\\Users\\33623\\Zotero\\storage\\DDPWH4SQ\\Cugini et al. - 2007 - The Cost of Customer Satisfaction A Framework for.pdf:application/pdf},
