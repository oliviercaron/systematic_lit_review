@article{oliver_disconfirmation_1985,
	title = {Disconfirmation Processes and Consumer Evaluations in Product Usage},
	volume = {13},
	issn = {01482963},
	abstract = {The article presents a study that operationalized and tested the role of two types of disconfirmation, inferential and overall, on the satisfaction of appetite suppressant users after first trial and repeat usage situations. Data were collected via three mailings to 800 members of a regional consumer panel in the first wave, attribute expectations regarding a new, heavily marketed appetite suppressant were collected. One month after the initial survey, a four-week supply of 24-hour capsules was mailed to each respondent. Following the four-week consumption period, a second mailing collected data regarding product usage, disconfirmation of expectations, feelings of satisfaction, and follow-up attitudes and intentions. Finally, a third mailing five months after the first survey also assessed satisfaction, attitudes, and intentions data. A total of 91 users and 262 nonusers provided complete responses to all three mailings. Only the user data set is relevant to this study. Results of the study suggest that satisfaction is primarily a function of overall disconfirmation and that the effect of inferred disconfirmation is mediated through the summary measure.},
	pages = {235--246},
