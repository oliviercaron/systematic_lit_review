	keywords = {{CUSTOMER} services, {EMPIRICAL} research, {TECHNOLOGICAL} innovations, {EUROPE}, {ORGANIZATIONAL} change, {PERSONNEL} management, {CUSTOMER} relations -- Management, {MARKETING} models, {STAKEHOLDERS}},
}

