	keywords = {{MANAGEMENT}, {RESEARCH}, {KNOWLEDGE} management, {QUALITY} control, {ORGANIZATIONAL} change, retention, {EMPLOYEE} retention, governance, Wikipedia, {INTELLECTUAL} cooperation, Peer production},
}

